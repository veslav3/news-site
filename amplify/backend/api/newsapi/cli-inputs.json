{
  "version": 1,
  "paths": {
    "/news": {
      "name": "/news",
      "lambdaFunction": "getnews",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/newsitem": {
      "name": "/newsitem",
      "lambdaFunction": "createnews",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "update"
        ]
      }
    }
  }
}